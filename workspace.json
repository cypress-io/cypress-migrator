{
  "version": 2,
  "projects": {
    "codemods": {
      "root": "libs/codemods",
      "sourceRoot": "libs/codemods/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/codemods/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/codemods"],
          "options": {
            "jestConfig": "libs/codemods/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/codemods",
            "tsConfig": "libs/codemods/tsconfig.lib.json",
            "packageJson": "libs/codemods/package.json",
            "main": "libs/codemods/src/index.ts",
            "assets": ["libs/codemods/*.md"]
          }
        }
      }
    },
    "translator": {
      "root": "apps/translator",
      "sourceRoot": "apps/translator",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "root": "apps/translator",
            "outputPath": "dist/apps/translator"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": {
            "buildTarget": "translator:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "translator:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": {
            "buildTarget": "translator:build:production"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/translator"],
          "options": {
            "jestConfig": "apps/translator/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/translator/**/*.{ts,tsx,js,jsx}"]
          }
        }
      }
    },
    "translator-e2e": {
      "root": "apps/translator-e2e",
      "sourceRoot": "apps/translator-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/translator-e2e/cypress.json",
            "devServerTarget": "translator:serve",
            "tsConfig": "apps/translator-e2e/tsconfig.json"
          },
          "configurations": {
            "production": {
              "devServerTarget": "translator:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/translator-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "translator-vscode": {
      "root": "libs/translator-vscode",
      "sourceRoot": "libs/translator-vscode/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/translator-vscode/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/translator-vscode"],
          "options": {
            "jestConfig": "libs/translator-vscode/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/translator-vscode",
            "tsConfig": "libs/translator-vscode/tsconfig.lib.json",
            "packageJson": "libs/translator-vscode/package.json",
            "main": "libs/translator-vscode/src/index.ts",
            "assets": ["libs/translator-vscode/*.md"]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/next"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "babel": true
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "css",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "translator"
}
