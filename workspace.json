{
  "version": 2,
  "projects": {
    "codemods": {
      "root": "libs/codemods",
      "sourceRoot": "libs/codemods/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/codemods/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/codemods"],
          "options": {
            "jestConfig": "libs/codemods/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/codemods",
            "tsConfig": "libs/codemods/tsconfig.lib.json",
            "packageJson": "libs/codemods/package.json",
            "main": "libs/codemods/src/index.ts",
            "assets": ["libs/codemods/*.md"]
          }
        }
      },
      "tags": []
    },
    "migrator": {
      "root": "apps/migrator",
      "sourceRoot": "apps/migrator",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "root": "apps/migrator",
            "outputPath": "dist/apps/migrator"
          },
          "configurations": {
            "production": {
              "buildTarget": "migrator:build:production",
              "outputPath": "dist/apps/migrator"
            }
          }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": {
            "buildTarget": "migrator:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "migrator:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": {
            "buildTarget": "migrator:build:production"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/migrator"],
          "options": {
            "jestConfig": "apps/migrator/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/migrator/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "deploy": {
          "executor": "@nrwl/workspace:run-commands",
          "outputs": [],
          "options": {
            "command": "nx affected --target=build --base=master --head=HEAD && nx build migrator --prod && cp -R dist/apps/migrator/next.config.js apps/migrator/.dist"
          }
        }
      },
      "tags": []
    },
    "migrator-e2e": {
      "root": "apps/migrator-e2e",
      "sourceRoot": "apps/migrator-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/migrator-e2e/cypress.json",
            "devServerTarget": "migrator:serve",
            "tsConfig": "apps/migrator-e2e/tsconfig.json"
          },
          "configurations": {
            "production": {
              "devServerTarget": "migrator:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/migrator-e2e/**/*.{js,ts}"]
          }
        }
      },
      "tags": [],
      "implicitDependencies": ["migrator"]
    },
    "migrator-new": {
      "projectType": "application",
      "root": "apps/migrator-new",
      "sourceRoot": "apps/migrator-new/src",
      "prefix": "cypress-dx",
      "targets": {
        "build": {
          "executor": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/migrator-new",
            "index": "apps/migrator-new/src/index.html",
            "main": "apps/migrator-new/src/main.ts",
            "polyfills": "apps/migrator-new/src/polyfills.ts",
            "tsConfig": "apps/migrator-new/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/migrator-new/src/assets",
              {
                "glob": "**/*",
                "input": "node_modules/monaco-editor",
                "output": "assets/monaco-editor"
              }
            ],
            "styles": ["apps/migrator-new/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/migrator-new/src/environments/environment.ts",
                  "with": "apps/migrator-new/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "executor": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "migrator-new:build:production"
            },
            "development": {
              "browserTarget": "migrator-new:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "executor": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "migrator-new:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/migrator-new/src/**/*.ts", "apps/migrator-new/src/**/*.html"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/migrator-new"],
          "options": {
            "jestConfig": "apps/migrator-new/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "migrator-new-e2e": {
      "root": "apps/migrator-new-e2e",
      "sourceRoot": "apps/migrator-new-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/migrator-new-e2e/cypress.json",
            "devServerTarget": "migrator-new:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "migrator-new:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/migrator-new-e2e/**/*.{js,ts}"]
          }
        }
      },
      "tags": [],
      "implicitDependencies": ["migrator-new"]
    },
    "migrator-state": {
      "projectType": "library",
      "root": "libs/migrator-state",
      "sourceRoot": "libs/migrator-state/src",
      "prefix": "cypress-dx",
      "targets": {
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/migrator-state"],
          "options": {
            "jestConfig": "libs/migrator-state/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/migrator-state/src/**/*.ts", "libs/migrator-state/src/**/*.html"]
          }
        }
      },
      "tags": []
    },
    "ng-codemods": {
      "projectType": "library",
      "root": "libs/ng-codemods",
      "sourceRoot": "libs/ng-codemods/src",
      "prefix": "cypress-dx",
      "targets": {
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ng-codemods"],
          "options": {
            "jestConfig": "libs/ng-codemods/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/ng-codemods/src/**/*.ts", "libs/ng-codemods/src/**/*.html"]
          }
        }
      },
      "tags": []
    },
    "tailwind": {
      "root": "libs/tailwind",
      "sourceRoot": "libs/tailwind",
      "projectType": "library",
      "targets": {},
      "tags": []
    },
    "vscode-migrator": {
      "root": "libs/vscode-migrator",
      "sourceRoot": "libs/vscode-migrator/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/vscode-migrator/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/vscode-migrator"],
          "options": {
            "jestConfig": "libs/vscode-migrator/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/vscode-migrator",
            "tsConfig": "libs/vscode-migrator/tsconfig.lib.json",
            "packageJson": "libs/vscode-migrator/package.json",
            "main": "libs/vscode-migrator/src/extension.ts",
            "assets": ["libs/vscode-migrator/*.md"]
          }
        }
      },
      "tags": []
    }
  }
}
