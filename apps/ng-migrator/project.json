{
  "projectType": "application",
  "root": "apps/ng-migrator",
  "sourceRoot": "apps/ng-migrator/src",
  "prefix": "cypress-dx",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:webpack-browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/ng-migrator",
        "index": "apps/ng-migrator/src/index.html",
        "main": "apps/ng-migrator/src/main.ts",
        "polyfills": "apps/ng-migrator/src/polyfills.ts",
        "tsConfig": "apps/ng-migrator/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/ng-migrator/src/favicon.ico",
          "apps/ng-migrator/src/assets",
          {
            "glob": "**/*",
            "input": "node_modules/monaco-editor",
            "output": "assets/monaco-editor"
          },
          {
            "input": "libs/markdown/src/lib",
            "glob": "**/*.md",
            "output": "assets"
          }
        ],
        "styles": ["apps/ng-migrator/src/styles.scss"],
        "scripts": ["node_modules/marked/lib/marked.js"],
        "customWebpackConfig": {
          "path": "apps/ng-migrator/webpack.config.js"
        }
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "8mb",
              "maximumError": "10mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/ng-migrator/src/environments/environment.ts",
              "with": "apps/ng-migrator/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@nrwl/angular:webpack-server",
      "configurations": {
        "production": {
          "browserTarget": "ng-migrator:build:production"
        },
        "development": {
          "browserTarget": "ng-migrator:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "ng-migrator:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["apps/ng-migrator/src/**/*.ts", "apps/ng-migrator/src/**/*.html"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/ng-migrator"],
      "options": {
        "jestConfig": "apps/ng-migrator/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
